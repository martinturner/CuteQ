<p id="notice"><%= notice %></p>

<h1>Listing Food Menus</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Food</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_menus.each do |food_menu| %>
      <tr>
        <td><%= food_menu.order_id %></td>
        <td><%= food_menu.food_id %></td>
        <td><%= link_to 'Show', food_menu %></td>
        <td><%= link_to 'Edit', edit_food_menu_path(food_menu) %></td>
        <td><%= link_to 'Destroy', food_menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food menu', new_food_menu_path %>
